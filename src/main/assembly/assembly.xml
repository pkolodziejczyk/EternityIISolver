
<assembly>
    <id>bin</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>true</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <unpack>false</unpack>
            <scope>runtime</scope>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <directory>AttachedFiles</directory>
            <outputDirectory></outputDirectory>
        </fileSet>
        <fileSet>
            <directory>Media</directory>
            <excludes>
                <exclude>**/*.xcf.*</exclude>
                <exclude>**/*.blend</exclude>
            </excludes>
        </fileSet>
    </fileSets>
</assembly>
